{
  "name": "chef",
  "version": "1.0",
  "description": "a chrome extension for hamayanhamayan",
  "permissions": [
    "clipboardWrite",
    "activeTab"
  ],
  "background":  {
    "scripts": ["background.js"],
    "persistant": false
  },
  "browser_action": {
      "name": "Copy"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Q"
      }
    }
  },
  "manifest_version": 2
}