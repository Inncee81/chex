{
  "name": "chef",
  "version": "1.0",
  "description": "a chrome extension for hamayanhamayan",
  "permissions": [
    "clipboardWrite",
    "activeTab"
  ],
  "background":  {
    "page": "background.html",
    "persistant": false
  },
  "browser_action": {
      "name": "Copy"
  },
  "commands": {
    "right-action": {
      "suggested_key": { 
        "default": "Ctrl+Right"
      },
      "description": "Ctrl+Right"
    },
    "left-action": {
      "suggested_key": { 
        "default": "Ctrl+Left"
      },
      "description": "Ctrl+Left"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Ri_execute_browser_actionght"
    }
  },
  "manifest_version": 2
}